{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "name": {
      "value": "${SERVICE_UI_NAME}"
    },
    "imageName": {
      "value": "${SERVICE_UI_IMAGE_NAME}"
    },
    "applicationInsightsName": {
      "value": "${APPLICATIONINSIGHTS_NAME}"
    },
    "containerAppsEnvironmentName": {
      "value": "${AZURE_CONTAINER_ENVIRONMENT_NAME}"
    },
    "containerRegistryName": {
      "value": "${AZURE_CONTAINER_REGISTRY_NAME}"
    },
    "accountingUri": {
      "value": "${SERVICE_ACCOUNTING_URI}"
    },
    "virtualCustomersUri": {
      "value": "${SERVICE_VIRTUAL_CUSTOMERS_URI}"
    },
    "ordersUri": {
      "value": "${SERVICE_ORDER_URI}"
    }
  }
}
